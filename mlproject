# MLproject
name: Electricity_Demand_Forecasting

docker_env:
  image: continuumio/miniconda3:latest

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      config: {type: str, default: "config/config.yaml"}
      run_name: {type: str, default: "default_run"}
    command: "python src/train_model.py --config {config} --run_name {run_name}"